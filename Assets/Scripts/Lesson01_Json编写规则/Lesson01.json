// 大括号包裹的就是一个对象
{
  // 冒号 代表的是 键值对的对应关系
  // 逗号 就是分割成员变量的间隔符
  // Json中的键 一定要用双引号包裹
  "name": "君哲",
  "age": 18,
  "sex": true,
  // 用于测试的浮点类型 支持浮点数
  "testF": 1.4,
  // 中括号代表数组
  "ids": [1, 2, 3],
  "students": [
    {
      "name": "小红",
      "age": 10,
      "sex": false
    },
    {
      "name": "小明",
      "age": 8,
      "sex": true
    },
    {
      "name": "小强",
      "age": 7,
      "sex": true
    }
  ],
  "home": {
    "address": "地球",
    "phone": "1234567890"
  },
  "son": null,
  // 字典的键会变成双引号 以后转换时要注意
  "dic": { "1": "123", "2": "234" }
}
